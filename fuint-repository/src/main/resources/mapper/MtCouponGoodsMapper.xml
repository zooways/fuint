<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fuint.repository.mapper.MtCouponGoodsMapper">
    <select id="getCouponGoods" resultType="com.fuint.repository.model.MtCouponGoods">
        select * from mt_coupon_goods o where o.COUPON_ID = #{couponId} and o.STATUS = 'A'
    </select>
</mapper>
