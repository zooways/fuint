<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fuint.repository.mapper.MtOrderAddressMapper">
    <select id="getOrderAddress" resultType="com.fuint.repository.model.MtOrderAddress">
        select * from mt_order_address o where o.ORDER_ID = #{orderId}
